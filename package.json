{
  "name": "vite-plugin-legacy-swc",
  "version": "1.1.0",
  "description": "Provides legacy browsers support for the production build with SWC",
  "type": "module",
  "main": "./dist/index.cjs",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.mjs",
      "require": "./dist/index.cjs"
    }
  },
  "files": [
    "dist"
  ],
  "directories": {
    "test": "tests"
  },
  "scripts": {
    "lint": "eslint --fix './**/*.{js,ts}'",
    "prepack": "unbuild",
    "release": "release-it",
    "test": "vitest"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/CyanSalt/vite-plugin-legacy-swc.git"
  },
  "keywords": [
    "vite",
    "vite-plugin",
    "legacy",
    "swc"
  ],
  "author": "CyanSalt",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/CyanSalt/vite-plugin-legacy-swc/issues"
  },
  "homepage": "https://github.com/CyanSalt/vite-plugin-legacy-swc#readme",
  "dependencies": {
    "@swc/core": "^1.5.24",
    "browserslist": "^4.23.0",
    "browserslist-to-esbuild": "^2.1.1",
    "core-js": "^3.37.1",
    "magic-string": "^0.30.10",
    "regenerator-runtime": "^0.14.1",
    "systemjs": "^6.15.1"
  },
  "devDependencies": {
    "@cyansalt/eslint-config": "^4.0.5",
    "@cyansalt/tsconfig": "^2.2.1",
    "@release-it/conventional-changelog": "^8.0.1",
    "@types/node": "^20.12.13",
    "acorn": "^8.11.3",
    "eslint": "^9.3.0",
    "picocolors": "^1.0.1",
    "release-it": "^17.3.0",
    "rollup": "^4.18.0",
    "typescript": "^5.4.5",
    "unbuild": "^2.0.0",
    "vite": "^5.2.12",
    "vitest": "^1.6.0"
  },
  "peerDependencies": {
    "vite": ">=4.0.0"
  }
}
