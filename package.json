{
  "name": "vite-plugin-legacy-swc",
  "version": "0.5.0",
  "description": "Provides legacy browsers support for the production build with SWC",
  "type": "module",
  "main": "./dist/index.cjs",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.mjs",
      "require": "./dist/index.cjs"
    }
  },
  "files": [
    "dist"
  ],
  "directories": {
    "test": "tests"
  },
  "scripts": {
    "lint": "eslint --ignore-path .gitignore --fix --ext .js,.ts .",
    "prepack": "unbuild",
    "release": "release-it",
    "test": "vitest"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/CyanSalt/vite-plugin-legacy-swc.git"
  },
  "keywords": [
    "vite",
    "legacy",
    "swc"
  ],
  "author": "CyanSalt",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/CyanSalt/vite-plugin-legacy-swc/issues"
  },
  "homepage": "https://github.com/CyanSalt/vite-plugin-legacy-swc#readme",
  "dependencies": {
    "@swc/core": "^1.3.92",
    "browserslist": "^4.21.5",
    "core-js": "^3.33.0",
    "magic-string": "^0.30.4",
    "regenerator-runtime": "^0.14.0",
    "systemjs": "^6.14.2"
  },
  "devDependencies": {
    "@cyansalt/eslint-config-preset": "^1.15.0",
    "@cyansalt/tsconfig": "^2.2.1",
    "@release-it/conventional-changelog": "^7.0.2",
    "@types/node": "^18.18.4",
    "release-it": "^16.2.1",
    "typescript": "^5.2.2",
    "unbuild": "^2.0.0",
    "vite": "^4.4.11",
    "vitest": "^0.34.6"
  },
  "peerDependencies": {
    "vite": ">=4.0.0"
  }
}
